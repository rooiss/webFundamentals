function a(x){
    for(var i=0; i<x.length; i++){
      if(x[i] > 5){
           x[i] = “Coding”;
      }
      else if(x[i] < 0){
           x[i] = “Dojo”;
      }
    }
    return x;
}
console.log(a([5,7,-1,4]))

function a(x){
    for(var i=0; i<x.length; i++){
        if(x[i] > 5){
            x[i] = "Coding";
        }
    }
    return x;
}
console.log(a([5,7,-1,4]))

function returnoddarray(){
    for(var i=;i<=255;i++){
        if (i%2!==0);
        x.push(i);
    }
}